Algoritmo "contede0a10"
Var
   contador, num, sal: Inteiro
Inicio

   contador <- 0
   escreva("Quer contar ate quanto? ")
   leia(num)
   Escreva("Qual sera o valor do salto? ")
   Leia(sal)
   Enquanto (contador <= num) faca
            EscrevaL(contador)
            contador <- (contador + sal)
   FimEnquanto
Fimalgoritmo